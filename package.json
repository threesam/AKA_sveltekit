    "@sanity/client": "^2.23.2",
    "@sanity/image-url": "^1.0.1",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.36",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "nodemailer": "^6.7.3",
    "stripe": "^8.215.0",
    "svelte-seo": "^1.4.0"
  }
}